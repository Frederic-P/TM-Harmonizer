{
  "name":"Trismegistos Harmonizer",
  "version": "1.0",
  "manifest_version": 2,

  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png" },
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action":{
    "default_popup": "popup.html",
    "default_title": "TM REPORT"
  },
}
